<% if session[:order_dishes]  %>
  <% if session[:order_dishes].any? %>
    <% session[:order_dishes].each do |order_dish| %>
      <%= render "orders/cart_row", id: order_dish["dish_id"],
        quantity: order_dish["quantity"], show_total: true %>
    <% end %>
  <% end %>
<% end %>
<% if session[:order_combos] %>
  <% if session[:order_combos].any? %>
    <% session[:order_combos].each do |order_combo| %>
      <%= render "orders/cart_row_combo", id: order_combo["combo_id"],
        quantity: order_combo["quantity"], show_total: true %>
    <% end %>
  <% end %>
<% end %>
